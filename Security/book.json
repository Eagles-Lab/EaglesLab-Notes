{
    "title": "英格网络实验室",
    "description": "英格网络实验室",
    "author": "Atopos",
    "output.name": "site",
    "language": "zh-hans",
    "root": ".",
    "links": {
        "sidebar": {
            "英格科技": "https://www.eagleslab.com"
        }
    },
    "plugins": [
        "search-plus",
        "flexible-alerts",
        "intopic-toc",
        "mermaid-hybrid",
        "expandable-chapters",
        "expandable-chapters-small",
        "-lunr",
        "-search"
    ],
    "pluginsConfig": {
        "search-plus": {
            "expandableIndices": true,
            "deepContent": "off"
        },
        "flexible-alerts": {
            "style": "flat",
            "comment": {
                "icon": "fa-comment",
                "label": "Comment"
            }
        },
        "intopic-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
            "mode": "nested",
            "maxDepth": 3,
            "isCollapsed": true,
            "isScrollspyActive": true,
            "label": "目录"
        },
        "expandable-chapters": {},
        "expandable-chapters-small": {}
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    }
}
