{
    "title": "英格网络实验室",
    "description": "英格网络实验室",
    "author": "AaronXu",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "英格科技": "https://www.eagleslab.com"
        }
    },
    "plugins": [
        "code",
        "-search",
        "search-pro",
        "-github",
        "splitter",
        "tbfed-pagefooter",
        "-donate",
        "-sharing",
        "-sharing-plus",
        "mermaid-gb3",
        "-prism",
        "highlight",
        "styles-less",
        "toggle-chapters",
        "multipart",
        "pageview-count",
        "ancre-navigation",
        "expandable-chapters-small",
        "theme-comscore",
        "-lunr"
    ],
    "pluginsConfig": {
        "code": {
            "copyButtons": true
        },
        "tbfed-pagefooter": {
            "copyright":"学前沿IT，到英格科技!",
            "modify_label": "本文发布时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ],
            "lang": {
                "flow": "typescript"
            }
        }
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    }
}